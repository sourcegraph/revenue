{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>sourcegraph/renovate-config"
  ],
  "packageRules": [
    {
      "matchFileNames": [
        "amp_demos/javascript/**"
      ],
      "groupName": "javascript demos"
    },
    {
      "matchFileNames": [
        "amp_demos/java/**"
      ],
      "groupName": "java demos"
    },
    {
      "matchFileNames": [
        "amp_demos/python/**"
      ],
      "groupName": "python demos"
    }
  ]
}
